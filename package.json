{
  "name": "travix",
  "private": true,
  "version": "0.7.10-beta",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "test:run": "vitest run",
    "prebuild": "node scripts/setup-env.js",
    "build": "vite build && node scripts/post-build.js",
    "build:clean": "node scripts/clear-cache.js && npm install && vite build && node scripts/post-build.js",
    "build:prod": "npm run build:clean",
    "generate-env": "node scripts/generate-env-js.js",
    "validate-env": "node scripts/validate-env-deployment.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "clean": "node scripts/clear-cache.js",
    "check-config": "node scripts/check-config.js",
    "verify-deploy": "node scripts/verify-deploy.js",
    "version:beta": "node scripts/auto-version.js",
    "version:minor": "node scripts/change-minor-version.js",
    "version:merge-main": "node scripts/merge-to-main-version.js",
    "pre-merge": "npm run lint && npm run build",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@heroicons/react": "2.2.0",
    "@react-google-maps/api": "^2.19.3",
    "@supabase/supabase-js": "2.50.2",
    "@tanstack/react-query": "5.83.0",
    "@tanstack/react-query-persist-client": "5.83.0",
    "dotenv": "17.2.1",
    "i18next": "25.2.1",
    "i18next-browser-languagedetector": "8.2.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "15.5.3",
    "react-router-dom": "7.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.2",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "24.0.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.0.11",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
